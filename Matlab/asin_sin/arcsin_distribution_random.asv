above=1;
below=1;
difference=[];
itterations=10000;
lines=
for a=1:lines
    for i=1:itterations
        if(sin(randi(itterations))>0)
            above=above+1;
        elseif(sin(randi(itterations))<0)
            below=below+1;
        end
        difference(i,)=above-below;
    end
end
scatter(1:itterations,difference,1,[1,0,0],'filled');
